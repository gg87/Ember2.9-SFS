<div id="posts">
    {{#each model as |post|}}
    <div class="panel panel-default" v-for="post in posts">
        <div class="panel-heading">
            {{post.title}}
        </div>
        <div class="panel-body">
            <p>{{post.body}}</p>
            {{#link-to 'user.detail' post.userId tagName="button"}}
                {{t "GO_TO_USER" }}
            {{/link-to}}
        </div>
    </div>
    {{/each}}
</div>
